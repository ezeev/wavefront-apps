{
    "id": "nginx",
    "name": "NGINX",
    "icon": "./nginx.png",
    "description": "Monitor NGINX stats",
    "installType": "telegraf",
    "overview": "./overview.md",
    "setup": "./setup.md",
    "metrics": {
        "prefixes": ["nginx."],
        "required": ["nginx.cpu.usage.idle","nginx.cpu.usage.user","nginx.cpu.usage.steal","nginx.cpu.usage.system","nginx.cpu.usage.guest"],
        "charts": ["./charts/chart1.json","./charts/chart2.json","./charts/chart3.json","./charts/chart4.json"]
    },
    "dashboards": [
        {
            "name": "NGINX Dummy Metrics",
            "description": "Monitor NGINX metrics for individual hosts",
            "url": "./dashboards/nginx-dummy.json"
        },
        {
            "name": "NGINX Dummy Metrics 2",
            "description": "Monitor NGINX metrics for individual hosts 2",
            "url": "./dashboards/nginx-dummy2.json"
        }
    ]
}
